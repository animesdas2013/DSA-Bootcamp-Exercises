// To calculate Fibonacci Series up to n numbers and the sum of it

import java.io.*;
import java.lang.*;
import java.util.*;

class GFG {
    
    static void FibonacciSeries(int n){
        int a = 0, b = 1, c;
        int sum = 0;
        System.out.println(a);
        System.out.println(b);
        
        for(int i = 2; i<n; i++){
            c = a+b;
            sum += c;
            System.out.println(c);
            a = b;
            b = c;
        }
    }
	public static void main (String[] args) {
	    Scanner sc = new Scanner(System.in);
	    int a = sc.nextInt();
	    FibonacciSeries(a);
	}
}
