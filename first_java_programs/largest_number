import java.io.*;
import java.lang.*;
import java.util.*;

class GFG {
    
    static void largestNum(int n, int m){
        if(n>m){
            System.out.println(n);
        }
        else{
            System.out.println(m);
        }
}
	public static void main (String[] args) {
	    Scanner sc = new Scanner(System.in);
	    int a = sc.nextInt();
	    int b = sc.nextInt();
	    largestNum(a,b);
	}
}
