import java.io.*;
import java.lang.*;
import java.util.*;

class GFG {
    
    static boolean palindromeofString(String n){
        int i = 0;
        int j = n.length()-1;
        while(i<j){
            if(n.charAt(i)!=n.charAt(j)){
                return false;
            }
            i++;
            j--;
        }
        return true;
    }
	public static void main (String[] args) {
	   // Scanner sc = new Scanner(System.in);
	   // String a = sc.nextLine();
	    String a = "ana";
	    System.out.println(palindromeofString(a));
	}
}
